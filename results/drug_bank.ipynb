    "from rdkit import Chem\n",
    "import pandas as pd\n",
    "\n",
    "# Load DrugBank SDF file\n",
    "def load_drugbank_sdf(sdf_path):\n",
    "    \"\"\"\n",
    "    Loads DrugBank SDF file, extracts compound information, and returns a pandas DataFrame.\n",
    "    \"\"\"\n",
    "    suppl = Chem.SDMolSupplier(sdf_path)\n",
    "    records = []\n",
    "    for mol in suppl:\n",
    "        if mol is None:\n",
    "            continue\n",
    "        dbid = mol.GetProp('_Name') if mol.HasProp('_Name') else None\n",
    "        name = mol.GetProp('GENERIC_NAME') if mol.HasProp('GENERIC_NAME') else None\n",
    "        smiles = Chem.MolToSmiles(mol) if mol is not None else None  # Generate SMILES\n",
    "        records.append({'dbid': dbid, 'name': name, 'smiles': smiles})\n",
    "    df = pd.DataFrame(records)\n",
    "    return df\n",
    "\n",
    "# Example Usage:\n",
    "sdf_path = '../data/structures.sdf'  # Replace with the actual path to your SDF file.\n",
    "df_drugbank = load_drugbank_sdf(sdf_path)\n",
    "\n",
    "# Print the first few rows of the DataFrame\n",

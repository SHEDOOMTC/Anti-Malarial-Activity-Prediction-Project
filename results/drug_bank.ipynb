    "\n",
    "# Save extracted data to CSV\n",
    "csv_path = '../data/drugbank_extracted.csv'\n",
    "df_drugbank.to_csv(csv_path, index=False)\n",
    "print(f\"Saved extracted DrugBank data to {csv_path}\")\n"
